<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.naver.b1.member.MemberMapper">
	
	<insert id="memberJoin" parameterType="com.naver.b1.member.MemberVO">
		insert into members values(#{id}, #{pw}, #{name}, #{email}) 
	</insert>
  
  	<select id="memberLogin" parameterType="com.naver.b1.member.MemberVO" resultType="com.naver.b1.member.MemberVO">
  		select * from members 
  		where id=#{id} and pw=#{pw}
  	</select>
  	
  	
  
  </mapper>
